				"jsCode": "// Comprehensive Test Results Report\nconst results = [];\nconst allItems = $input.all();\n\nconsole.log(`Processing ${allItems.length} test results...`);\n\nfor (const item of allItems) {\n  // Get test name from the Set node that added it\n  const nodeName = item.json.test_name || 'Unknown Test';\n  const hasError = !!item.json.error;\n  const errorMessage = item.json.error?.message || null;\n  \n  // Determine if the test passed\n  const passed = !hasError && (item.json.id || item.json.length >= 0 || item.json.accounts || item.json.statuses || item.json.hashtags || item.json.instance || item.json.preferences || Object.keys(item.json).length > 2);\n  \n  results.push({\n    test: nodeName,\n    status: passed ? '‚úÖ PASS' : '‚ùå FAIL',\n    error: errorMessage,\n    hasData: !hasError && Object.keys(item.json).length > 1,\n    dataKeys: Object.keys(item.json).filter(k => k !== 'test_name' && k !== 'error').slice(0, 5).join(', ')\n  });\n}\n\nconst passed = results.filter(r => r.status === '‚úÖ PASS').length;\nconst failed = results.filter(r => r.status === '‚ùå FAIL').length;\nconst successRate = ((passed / results.length) * 100).toFixed(2);\n\n// Group results by status\nconst passedTests = results.filter(r => r.status === '‚úÖ PASS');\nconst failedTests = results.filter(r => r.status === '‚ùå FAIL');\n\nconsole.log(`\\n${'='.repeat(60)}`);\nconsole.log('MASTODON PLUGIN TEST RESULTS');\nconsole.log('='.repeat(60));\nconsole.log(`Total Tests: ${results.length}`);\nconsole.log(`Passed: ${passed} (${successRate}%)`);\nconsole.log(`Failed: ${failed}`);\nconsole.log('='.repeat(60));\n\nif (failedTests.length > 0) {\n  console.log('\\nFailed Tests:');\n  failedTests.forEach(t => {\n    console.log(`  ${t.status} ${t.test}`);\n    if (t.error) console.log(`     Error: ${t.error}`);\n  });\n}\n\nif (passedTests.length > 0) {\n  console.log('\\nPassed Tests:');\n  passedTests.forEach(t => {\n    console.log(`  ${t.status} ${t.test}`);\n    if (t.dataKeys) console.log(`     Data: ${t.dataKeys}`);\n  });\n}\n\nconsole.log('\\n' + '='.repeat(60));\n\nreturn [\n  {\n    json: {\n      summary: {\n        total: results.length,\n        passed: passed,\n        failed: failed,\n        success_rate: `${successRate}%`,\n        timestamp: new Date().toISOString(),\n        verdict: failed === 0 ? 'üéâ ALL TESTS PASSED!' : `‚ö†Ô∏è  ${failed} TEST(S) FAILED`\n      },\n      passed_tests: passedTests,\n      failed_tests: failedTests,\n      all_results: results\n    }\n  }\n];",
